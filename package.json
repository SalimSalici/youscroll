{
  "name": "youscroll",
  "version": "0.0.0",
  "description": "A simple and lightweight javascript library to smoothly scroll a page to a desired location.",
  "main": "src/youscroll.js",
  "scripts": {
    "build-min": "uglifyjs src/youscroll.js -o dist/youscroll.min.js --compress --mangle --comments",
    "build-beau": "uglifyjs src/youscroll.js -o dist/youscroll.js --beautify --comments all",
    "build": "npm run build-min && npm run build-beau",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SalimSalici/youscroll.git"
  },
  "keywords": [
    "javascript",
    "smooth",
    "scroll"
  ],
  "author": "Salim Salici",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SalimSalici/youscroll/issues"
  },
  "homepage": "https://github.com/SalimSalici/youscroll#readme",
  "devDependencies": {
    "uglify-js": "^2.8.22"
  }
}
